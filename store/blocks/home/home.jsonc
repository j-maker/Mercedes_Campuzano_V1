  "rich-text#salesDisccount": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "LLEVA OBSEQUIO POR COMPRAS IGUALES O SUPERIORES A $300.000",
      "blockClass": "salesDisccount"
    }
