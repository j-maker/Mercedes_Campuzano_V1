  "rich-text#terms-use-content-1-subtitle-right": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "**1. Las partes.**",
      "blockClass": "footer-terms-content-subtitles-terms-use"
    }
  },
  "rich-text#terms-use-content-1-right": {
    "props": {
