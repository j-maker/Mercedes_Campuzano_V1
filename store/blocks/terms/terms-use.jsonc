  "rich-text#terms-use-content-9-left": {
    "props": {
      "textAlignment": "JUSTIFY",
      "textPosition": "JUSTIFY",
      "text": "La factura se remitirá junto con el producto adquirido a la dirección mencionada por el cliente.",
      "blockClass": "footer-terms-content-terms-use"
    }
